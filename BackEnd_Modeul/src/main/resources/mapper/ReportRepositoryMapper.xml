<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.modeul.web.repository.ReportRepository">

	<resultMap id="reportResultMap" type="Report">
		<result property="Id" column="id" />
		<result property="regDate" column="reg_date" />
		<result property="stuffId" column="stuff_id" />
		<result property="memberId" column="member_id" />
		<result property="detail" column="detail" />
		<result property="progress" column="progress" />
		<result property="reply" column="reply" />
	</resultMap>



	<select id="findbyProgress" resultMap="reportResultMap">
		select *
		from report
		where progress = #{progress}
	</select>
	
	<select id="findAll" resultMap="reportResultMap">
		select *
		from report
	</select>
	
</mapper>